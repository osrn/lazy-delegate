{
    "apps": [{
        "name": "lazy-delegate",
        "description": "Python Solar Node Monitoring with Discord Notification Facility",
        "version": "0.54b",
        "author": "osrn <osrnnetwork@gmail.com> (https://osrn.github.io)",
        "repository": "https://github.com/osrn/lazy-delegate.git",
        "homepage": "https://github.com/osrn/lazy-delegate",
        "license": "MIT",
        "bugs": {
            "url": "https://github.com/osrn/lazy-delegate/issues"
        },
        "script": "src/lazydel.py",
        "exec_interpreter": ".venv/bin/python3",
        "interpreter_args": "-u",
        "instances": 1,
        "exec_mode": "fork",
        "autorestart": false,
        "watch":false,
        "log_date_format": "YYYY-MM-DD HH:mm Z",
        "timestamp": "YYYY-MM-DD HH:mm Z"
    }]
}
